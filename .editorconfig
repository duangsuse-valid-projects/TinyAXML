root = true

[*]
insert_final_newline = true
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8

[*.java]
indent_size = 4

[*.json]
indent_size = 4

[*.xml]
insert_final_newline = false

[*.gradle]
indent_size = 4

[gradlew]
indent_size = 4

[gradlew.bat]
end_of_line = crlf

[Makefile]
indent_style = tab
